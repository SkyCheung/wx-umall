/* pages/index/index.wxss */
.index-page{
  background-color:#eeeeee;
}
.p1th .swiper{
  height:360rpx;
}
.p1th .swiper swiper-item{
  height:360rpx;
}
.p1th .swiper swiper-item .img{
  width:100%;
  height:360rpx;
  display: block;
}
.p2th{
  padding:16rpx 56rpx;
  background-color: #fff;
}
.p2th .input-item{
  padding:10rpx 23rpx  10rpx 76rpx;
  border:1px solid #dcdcdc;
  border-radius:20rpx;
  background:url("data:image/jpg;base64,");
  background-size:32rpx 32rpx;
  background-position:25rpx center;
  background-repeat: no-repeat;
}
.p2th .input-item input{
  height:32rpx;
  line-height: 32rpx;
  font-size: 25rpx;
}
.p2th .classify-item{
  display: flex;
  flex-direction:row;
}
.p2th .classify-item .item{
  width: 128rpx;
}
.p2th .classify-item .item .img-item image{
  width: 80rpx;
  height: 82rpx;
  margin: 18rpx auto;
  display: block;
}
.p2th .classify-item .item .txt-item{
  font-size: 27rpx;
  text-align: center;
}
.p3th{
  background-color: #fff;
}
.p3th .title-item{
  text-align: center;
  position: relative;
}
.p3th .title-item .title{
  display: inline-block;
  font-size: 30rpx;
  color:#fe051d;
  margin-left: 13rpx;
  vertical-align: middle;
}
.p3th .title-item .new-icon{
  display: inline-block;
  width:30rpx;
  height: 30rpx;
  vertical-align: middle;
  background: url("data:image/jpg;base64,");
  background-size:30rpx 30rpx;
  background-repeat: no-repeat;
}
.p3th .title-item .arrow-icon{
  width: 22rpx;
  height: 24rpx;
  position:absolute;
  right:46rpx;
  top:17rpx;
  z-index: 1;
  background: url("data:image/jpg;base64,");
  background-size:22rpx 24rpx;
  background-repeat: no-repeat;
}
.p3th .list{
  padding-left: 24rpx;
  margin-top: 25rpx;
  overflow: hidden;
}
.p3th .list .item{
  width: 336rpx;
  float: left;
  margin: 0rpx 24rpx 30rpx 0rpx;

}
.p3th .list .item .image-item image{
  width: 336rpx;
  height: 336rpx;
  display: block; 
  border: 2rpx solid #dcdcdc;
  border-radius: 20rpx;
}
.p3th .list .txt-item{
 
 margin-top: 10rpx;
 overflow: hidden;
}
.p3th .list .txt-item .item-1{
  padding:0rpx 6rpx;
  max-width: 78rpx;
  float: left;
  background-color: #fe0c24;
  display: inline-block;
  color:#fff;
  font-size: 25rpx;
  border-radius: 10rpx;

}
.p3th .list .txt-item .item-2{
  float: left;
  margin-left: 10rpx;
  max-width: 242rpx;
}
.p3th .list .txt-item .item-2 .txt-1{
  font-size: 25rpx;
}
.p3th .list .txt-item .item-2 .txt-2{
  font-size: 25rpx;
  margin-top: 5rpx;
}
.p3th .list .txt-item .item-2 .txt-3{
  font-size: 30rpx;
  margin-top: 12rpx;
  color:#d91f1f;
}

