
.p1th{
  padding:30rpx 26rpx 0rpx 26rpx;
}
.p1th .wrap{
  position:relative;
}
.p1th .title-item{
   border:2rpx solid #b5b5b5;
   border-radius: 20rpx;
  
}
.p1th .title-item .txt-1{
  text-align: center;
  padding: 13rpx 0rpx;
  border-top-left-radius: 20rpx;
  border-top-right-radius: 20rpx;
  position:relative;
  background-color: #fff;
  font-size: 33rpx;
  color:#1b1b1b;
}
.p1th .title-item .txt-1 .txt-2{
  position:absolute;
  left: 20rpx;
  top:22rpx;
  z-index: 1;
  color:#a0a0a0;
  font-size: 20rpx;
}
.p1th .title-item .txt-1 .icon{
  width: 24rpx;
  height: 22rpx;
  display: inline-block;
  margin-left: 10rpx;
  background-image: url("data:image/jpg;base64,");
  background-size: 24rpx 22rpx;
}
.p1th .title-item  .category-list{
  overflow: hidden;
   border-top: 1px solid #b3b3b3;
   background: rgba(255, 255, 255, .9);
   
   
}

.p1th .title-item  .category-list .item text{
  font-size: 24rpx;
  color:#535353;
 
}
.p1th .arrow-item{
  text-align: center;
  padding-bottom:15rpx;
  background: rgba(255, 255, 255, .9);
  border-bottom-left-radius: 20rpx;
  border-bottom-right-radius: 20rpx;
}
.p1th .arrow-item .icon{
  width: 24rpx;
  height: 22rpx;
  display: inline-block;
  margin-left: 10rpx;
  background-image: url("data:image/jpg;base64,");
  background-size: 24rpx 22rpx;
}
.p1th .item-2{
  width: 100%;
  position: absolute;
  left: 0;
  top:0;
  z-index: 1;
}
.p1th .item-2  .category-list{
   padding:10rpx 60rpx 20rpx 60rpx;
}
.p1th .item-2  .category-list .item{
  width: 20%;
  float: left;
  padding-bottom:5rpx;
}

.p1th .item-3{
  width: 100%;
  position: absolute;
  left: 0;
  top:0;
  z-index: 2;
}
.p1th .item-3  .category-list{
  padding:10rpx 0rpx 20rpx 0rpx; 
  display: flex;
  flex-wrap:wrap;
}
.p1th .item-3  .category-list .item{
   width: 25%; 
  text-align: center;
  padding-bottom:5rpx;
}

.p2th{
  background-color: #fff;
}

.p2th .list{
  padding-left: 24rpx;
  margin-top: 25rpx;
  overflow: hidden;
}
.p2th .list .item{
  width: 336rpx;
  float: left;
  margin: 0rpx 24rpx 30rpx 0rpx;

}
.p2th .list .item .image-item image{
  width: 336rpx;
  height: 336rpx;
  display: block; 
  border: 2rpx solid #dcdcdc;
  border-radius: 20rpx;
}
.p2th .list .txt-item{
 
 margin-top: 10rpx;
 overflow: hidden;
}
.p2th .list .txt-item .item-1{
  padding:0rpx 6rpx;
  max-width: 78rpx;
  float: left;
  background-color: #fe0c24;
  display: inline-block;
  color:#fff;
  font-size: 25rpx;
  border-radius: 10rpx;

}
.p2th .list .txt-item .item-2{
  float: left;
  margin-left: 10rpx;
  max-width: 242rpx;
}
.p2th .list .txt-item .item-2 .txt-1{
  font-size: 25rpx;
}
.p2th .list .txt-item .item-2 .txt-2{
  font-size: 25rpx;
  margin-top: 5rpx;
}
.p2th .list .txt-item .item-2 .txt-3{
  font-size: 30rpx;
  margin-top: 12rpx;
  color:#d91f1f;
}

